local Tab = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


Tab:AddToggle({
	Name = "Auto Claim Free Spin",
	Default = false,
	Callback = function(ACFS)
		_G.autospin = ACFS;
	end    
})

task.spawn(function()
	while wait() do
		if _G.autospin then
			game:GetService("ReplicatedStorage").System.SystemDailyLottery.Spin:InvokeServer()
			wait(5)
		end
	end
end)

Tab:AddButton({
	Name = "Redeem All Codes",
	Callback = function()
		game:GetService("ReplicatedStorage").ServerMsg.Code:InvokeServer("DRAGON")
		wait(1)
		game:GetService("ReplicatedStorage").ServerMsg.Code:InvokeServer("5KLIKES")
		wait(1)
		game:GetService("ReplicatedStorage").ServerMsg.Code:InvokeServer("UPDATE2")
		wait(1)
		game:GetService("ReplicatedStorage").ServerMsg.Code:InvokeServer("ONEPIECE")
		wait(1)
		game:GetService("ReplicatedStorage").ServerMsg.Code:InvokeServer("MASK")
		wait(1)
		game:GetService("ReplicatedStorage").ServerMsg.Code:InvokeServer("RUSHRUSH")
		wait(1)
  	end    
})

local Tab = Window:MakeTab({
	Name = "Credits",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

